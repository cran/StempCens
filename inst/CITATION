  author   = c(person("Katherine L. Valeriano", role = "aut"),
               person("Victor H. Lachos", role = "aut"),
               person("Larissa A. Matos", role = "aut")),
  year     = "2025",
  note     = "R package version 1.2.0",
  url      = "https://CRAN.R-project.org/package=StempCens",

  textVersion =
  paste("Valeriano K, Lachos VH and Matos L (2025).", "StempCens: Spatio-temporal estimation and prediction for censored/missing responses.", "R package version 1.2.0.", "<URL: https://CRAN.R-project.org/package=StempCens >."
  )
)
